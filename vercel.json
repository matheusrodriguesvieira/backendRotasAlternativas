{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.6.0"
      }
    },
    "rewrites": [
      { "source": "/:path*",  "destination": "api/index.php?path=:path*" }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          
        ]
      }
    ]
  }
  